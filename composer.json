{
    "name": "muhammed-alkhudiry/lara-helpers",
    "description": "lara-helpers",
    "keywords": [
        "muhammed-alkhudiry",
        "lara-helpers"
    ],
    "homepage": "https://github.com/muhammedalkhudiry/lara-helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammed",
            "email": "muhamdkudiry@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "geniusts/hijri-dates": "^1.1",
        "illuminate/http": "^8.33",
        "illuminate/support": "^8.33"
    },
    "require-dev": {
    },
    "autoload": {
        "files": [
            "src/util/helpers.php"
        ],
        "psr-4": {
            "Muhammedalkhudiry\\LaraHelpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muhammedalkhudiry\\LaraHelpers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
